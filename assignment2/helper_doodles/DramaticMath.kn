Doodle AddNumbers(a, b) {
    Let sum = a + b;
    Return sum;  // Just Saying: Clean return, no drama here.
}

Doodle Multiply(a, b) {
    Let result = a * b;
    Return result;  // Just Saying: Math magic, but boring.
}

